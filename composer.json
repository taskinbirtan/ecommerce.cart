{
    "name": "taskinbirtan/ecommerce.cart",
    "autoload": {
        "psr-4": {
            "Taskinbirtan\\EcommerceCart\\": "src/",
            "Taskinbirtan\\EcommerceCart\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Birtan Taşkın",
            "email": "taskinbirtan@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Taskinbirtan\\EcommerceCart\\Providers\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Taskinbirtan\\EcommerceCart\\Facades\\Cart"
            }
        }
    },
    "require-dev": {
        "laravel/framework": "^10.0",
        "pestphp/pest": "^2.36",
        "pestphp/pest-plugin-laravel": "^2.4",
        "orchestra/testbench": "^8.36"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
